{
  "name": "corona-testing-application",
  "version": "1.0.0",
  "author": "Adam Bennett, Moe Aydin",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start-web": "npm run start --prefix corona-testing-front",
    "build-web": "npm run build --prefix corona-testing-front",
    "start-api-prod": "npm run start --prefix corona-testing-server",
    "start-api-dev": "npm run start:dev --prefix corona-testing-server",
    "application-start-dev": "npm run p \"npm run start-web\" \"npm run start-api-dev\"",
    "application-start": "npm run build-web & npm run p \"npm run start-web\" \"npm run start-api-prod\"",
    "browser-check": "npm run browsers --prefix corona-testing-front",
    "p": "parallelshell"
  },
  "dependencies": {
    "parallelshell": "file:parallelshell"
  },
  "devDependencies": {
    "@types/node": "^14.14.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
